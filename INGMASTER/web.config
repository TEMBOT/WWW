<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <remove name="LocalSqlServer" />
    <add name="LocalSqlServer" connectionString="Server=SQLEXPRESS;Database=IMS;User ID=Администратор;Password=2188" providerName="System.Data.SqlClient" />
    <add name="MSSQLSERVER" connectionString="Data Source=MSSQLSERVER;Initial Catalog=IMS;User ID=Администратор;Password=2188" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <!--
    Описание изменений web.config см. по адресу http://go.microsoft.com/fwlink/?LinkId=235367.

    Следующие атрибуты можно установить с помощью тега <httpRuntime>.
      <system.Web>
        <httpRuntime targetFramework="4.7.1" />
      </system.Web>
  -->
  <system.web>
    <authentication mode="Forms">
      <forms cookieless="AutoDetect" name=".OpenAuthProviders" protection="Validation" requireSSL="false" slidingExpiration="true" />
    </authentication>
    <compilation debug="true" targetFramework="4.7.1">
      <assemblies>
        <add assembly="System.DirectoryServices, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="stdole, Version=7.0.3300.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Net.Http, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Net.Http.WebRequest, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.6.1" />
    <pages>
      <namespaces>
        <add namespace="System.Web.Optimization" />
        <add namespace="Microsoft.AspNet.Identity" />
      </namespaces>
      <controls>
        <add assembly="Microsoft.AspNet.Web.Optimization.WebForms" namespace="Microsoft.AspNet.Web.Optimization.WebForms" tagPrefix="webopt" />
      </controls>
    </pages>
    <membership defaultProvider="Администратор">
      <providers>
        <!--
        Членство ASP.NET отключено в этом шаблоне. Подробности о поддержке членства ASP.NET этим шаблоном см. на странице https://go.microsoft.com/fwlink/?LinkId=301889
        -->
        <clear />
        <add name="Администратор" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" connectionStringName="LocalSqlServer" enablePasswordReset="true" enablePasswordRetrieval="true" passwordFormat="Encrypted" requiresQuestionAndAnswer="true" requiresUniqueEmail="true" applicationName="Администраторы" />
        <add name="Администраторы" type="System.Web.Security.SqlMembershipProvider, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" connectionStringName="MSSQLSERVER" enablePasswordReset="false" enablePasswordRetrieval="false" passwordFormat="Clear" requiresQuestionAndAnswer="false" requiresUniqueEmail="false" applicationName="Администраторы" description="Администраторы" />
      </providers>
    </membership>
    <profile defaultProvider="dw" enabled="true">
      <providers>
        <!--
        Профиль членства ASP.NET отключен в этом шаблоне. Подробности о поддержке членства ASP.NET этим шаблоном см. на странице https://go.microsoft.com/fwlink/?LinkId=301889.
        -->
        <clear />
        <add name="dw" type="System.Web.Profile.SqlProfileProvider, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" connectionStringName="LocalSqlServer" />
      </providers>
      <properties>
        <add allowAnonymous="true" defaultValue="" name="Администраторы" readOnly="false" serializeAs="ProviderSpecific" type="System.Char" />
        <group name="Админ" />
      </properties>
    </profile>
    <roleManager defaultProvider="sxd" enabled="true">
      <!--
          Роль членства ASP.NET отключена в этом шаблоне. Подробности о поддержке членства ASP.NET этим шаблоном см. на странице https://go.microsoft.com/fwlink/?LinkId=301889.
        -->
      <providers>
        <clear />
        <add name="sxd" type="System.Web.Security.SqlRoleProvider, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" connectionStringName="MSSQLSERVER" />
      </providers>
    </roleManager>
    <!--
            Если выполняется развертывание в облачной среде с несколькими экземплярами веб-сервера,
            следует изменить состояние сеанса с "InProc" на "Custom". Также
            измените строку подключения "DefaultConnection", чтобы выполнять подключение к экземпляру
            сервера SQL Server (в том числе SQL Azure и SQL Compact), а не SQL Server Express.
      -->
    <sessionState customProvider="DefaultSessionProvider" mode="Off">
      <providers>
        <add name="DefaultSessionProvider" type="System.Web.Providers.DefaultSessionStateProvider, System.Web.Providers, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" connectionStringName="DefaultConnection" />
      </providers>
    </sessionState>
        <identity impersonate="true" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
    <defaultDocument>
      <files>
        <clear />
        <add value="default.aspx" />
        <add value="Default.htm" />
        <add value="Default.asp" />
        <add value="index.htm" />
        <add value="index.html" />
        <add value="iisstart.htm" />
        <add value="index.php" />
      </files>
    </defaultDocument>
    <directoryBrowse enabled="true" showFlags="Date, Time, Size, Extension, LongDate" />
    <security>
      <authorization>
        <add accessType="Allow" users="vstb" />
      </authorization>
    </security>
        <tracing>
            <traceFailedRequests>
                <add path="*">
                    <traceAreas>
                        <add provider="ASP" verbosity="General" />
                        <add provider="ASPNET" areas="Infrastructure,Module,Page,AppServices" verbosity="General" />
                        <add provider="ISAPI Extension" verbosity="Verbose" />
                        <add provider="WWW Server" areas="Authentication,Security,Filter,StaticFile,CGI,Compression,Cache,RequestNotifications,Module,FastCGI,WebSocket" verbosity="Verbose" />
                    </traceAreas>
                    <failureDefinitions statusCodes="100-999" />
                </add>
                <add path="*.aspx">
                    <traceAreas>
                        <add provider="ASP" verbosity="Verbose" />
                        <add provider="ASPNET" areas="Infrastructure,Module,Page,AppServices" verbosity="Verbose" />
                        <add provider="ISAPI Extension" verbosity="Verbose" />
                        <add provider="WWW Server" areas="Authentication,Security,Filter,StaticFile,CGI,Compression,Cache,RequestNotifications,Module,FastCGI,WebSocket" verbosity="Verbose" />
                    </traceAreas>
                    <failureDefinitions statusCodes="500" />
                </add>
            </traceFailedRequests>
        </tracing>
  </system.webServer>
